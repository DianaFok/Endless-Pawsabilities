<!-- Login page-->
<section class="section-md bg-white text-center">
  <div class="shell">
    <div class="range range-sm-center spacing-55">
      <div class="cell-xs-12">
        <h2>Admin Login</h2>
      </div>
      <div class="cell-sm-9 cell-md-7 cell-lg-6">
        <div class="inset-sm-25">
          <!-- RD Mailform-->
          <form class="rd-mailform form-label-centered" action="/adminlogin" method="post">
            <div class="form-group">
              <label class="form-label-outside" for="login-email">E-mail</label>
              <input id="login-email" type="text" class="form-control" name="email" data-constraints="@Email @Required">
            </div>
            <div class="form-group">
              <label class="form-label-outside" for="login-password">Password</label>
              <input type="password" id="login-password" class="form-control" name="password"  data-constraints="@Required">
            </div>
            <button class="btn btn-tan-hide btn-block" type="submit">Enter</button>
            <span href="/signup">Need an account? Please contact administration to set up an admin account.</span>
          </form>
        </div>
      </div>
    </div>
  </div>
