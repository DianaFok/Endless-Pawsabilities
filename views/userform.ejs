    <style>
      form{
        display: flex;
        flex-direction: column;
        width: 50%;
        margin-top: 5rem;
        margin-bottom: 5rem;
        margin-left: auto;
        margin-right: auto;
      }
      input {
        font-size: 2rem;
        margin-bottom: 0.5rem;
      }
    </style>
    <form action="/submitForm" method="post">
        <input type="text" name="userName" value="" placeholder="Name of applicant">
        <input style="display:none;" type="text" name="userEmail" value="<%= user.local.email %>" placeholder="">
        <input type="text" name="userNumber" value="" placeholder="Contact Information">
        <input type="text" name="userAddress" value="" placeholder="Address">
        <input type="text" name="petName" value="" placeholder="Name of animal">
        <input type="text" name="ownPets" value="" placeholder="Do you own any pets">
        <input type="text" name="schedule" value="" placeholder="What is your work schedule">
        <input type="text" name="vet" value="" placeholder="Do you have a vet">
        <input type="text" name="home" value=""  placeholder="Do you live in apartment or own home">
        <input type="text" name="roomates" value="" placeholder="Do you live with other people">
        <input type="text" name="behavior" value="" placeholder="How would you deal with behavioral issues">
        <input type="text" name="reference" value="" placeholder="Please provide a reference">
        <input type="submit" value="Submit">
    </form>

